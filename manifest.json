{
    "manifest_version": 2,
    "name":    "Grade Notifier",
    "version": "1.0",

    "content_scripts": [{
        "matches": ["file:///*"],
        "js":      ["content.js"]
    }],

    "browser_action": {
        "default_title": "Grade Notifier"
    },

    "permissions": ["activeTab"]
}